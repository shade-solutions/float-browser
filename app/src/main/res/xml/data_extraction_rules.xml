<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="cache" />
        <exclude domain="no_backup" />
        <include domain="sharedpref" path="FloatBrowserPrefs.xml"/>
    </cloud-backup>
    
    <device-transfer>
        <!-- Allow basic preferences transfer -->
        <include domain="sharedpref" path="FloatBrowserPrefs.xml"/>
    </device-transfer>
</data-extraction-rules>